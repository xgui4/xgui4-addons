{
    "browser_specific_settings": {
        "gecko": {
        "id": "@xgui4-addon",
        "data_collection_permissions": {
            "required" : ["none"]
            }
        }
    },
    "manifest_version": 3,
    "name": "Xgui4 Addon",
    "description": "My first ever Browser Extension",
    "version": "0.0.0.1",
    "action": {
        "default_popup": "ui.html",
        "default_icon": "icon.png"
    },
    "icons" :{
        "94" : "icon/icon-94.png"
    },
    "permissions": ["activeTab"],
    "content_scripts": [
    {
      "matches": ["https://xgui4.github.io/*"],
      "js": ["banner.js"]
    }
    ]
}